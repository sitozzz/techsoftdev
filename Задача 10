#include <iostream>
#include <conio.h>
#include <cmath>

using namespace std;

int main()
{
	int counter = 0;
	int64_t sum = 0;
	long k = 2000000;
	for (int i = 0; i<k; i++)
	{
		if ((i % 9 == 0) || (i % 2 == 0))
		{
			continue;
		}
 
		for (int j = 1; j <= i; j++)
		{
			if (i % j == 0)
			{
				counter++;
				if (counter > 2)
				{
					break;
				}
			}
		}
		if (counter == 2)
		{
			
			sum = sum + i;
			counter = 0;
			
		
			
		}
		else { counter = 0; }
		
	}
	cout << sum+2;
	_getch();
}
